{
  "name": "cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "herokuapp": "npx cypress run --spec 'cypress/e2e/herokuapp.cy.js'",
    "config-2": "npx cypress open --config-file cypress_2.config.js",
    "cy:dashboard": "npx cypress run --record --key 69d9d983-6df7-46d9-9308-e4d8c8074d63 --config-file cypress_2.config.js --spec cypress/e2e/herokuapp.cy.js",
    "spec1": "npx cypress run --spec 'cypress/e2e/spec.cy.js' --browser chrome",
    "spec2": "npx cypress run --spec 'cypress/e2e/spec_2.cy.js' --browser electron",
    "both_specs": "npm run --record --key 69d9d983-6df7-46d9-9308-e4d8c8074d63 spec1 && npm run spec2"
  },
  "author": "khusaenova",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.6.1"
  }
}
